{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dirbbox",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Este aplicativo usar√° opcionalmente o Face ID ou Touch ID para salvar o login"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "usesIcloudStorage": true
    }
  }
}